@model IEnumerable<Domain.Entities.contract>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/GlobalView.cshtml";
}

  
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table" id="datatables">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.costContract)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dateSending)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.endDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.etat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.startDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.insured_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.policy_id)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.costContract)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dateSending)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.endDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.etat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.startDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.insured_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.policy_id)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                @Html.ActionLink("Details", "Details", new { id = item.id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.id })
            </td>
        </tr>

    }

</table>
